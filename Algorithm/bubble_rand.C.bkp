#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main()
{
  int i,r,n;
  int temp;
  int A[100000];
  n = 10000;

  for(r = 0; r < n; r++)
  {
    A[r] = rand();
  }

  for (i = 1; i < 10000; i++)
  {
    for (r = 0; r < n-i; r++)
    {
      if( A[r]>A[r+1])
      {
        temp = A[r];
        A[r] = A[r+1];
        A[r+1] = temp;
      }
    }
  }

  for (r = 0; r < n; r++){
    printf("%d \n", A[r]);
  }
  return 0;
}
